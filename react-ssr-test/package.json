{
  "name": "react-ssr-test",
  "version": "1.0.0",
  "description": "React SSR with Rspack example",
  "scripts": {
    "dev": "pnpm run build:client && pnpm run build:server && node dist/server/server.js",
    "build:client": "rspack build --config rspack.client.js",
    "build:server": "rspack build --config rspack.server.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "express": "^4.18.3"
  },
  "devDependencies": {
    "@rspack/cli": "^0.5.7",
    "@rspack/core": "^0.5.7",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/express": "^4.17.21",
    "typescript": "^5.4.2"
  }
} 